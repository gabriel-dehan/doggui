
<b> Nom du chien : </b><%= dog.nickname %>
<br>
<b> Adresse : </b> <%= dog.address %>
<br>
<% if dog.picture_url %>
  <img src="<%= dog.picture.url  %>" width="200 ", height="90">
<%else%>

  <%= image_tag("dog.png", height: 90, width: 225) %>
<% end %>
<br>
<%= link_to "Voir ce chien", dog_path(dog), class: "btn btn-warning"  %>



